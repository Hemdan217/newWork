{
  "name": "checkurl",
  "displayName": "Check URL",
  "version": "0.0.1",
  "description": "A basic Demo extension.",
  "packageManager": "npm@9.5.1",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "firefox": "plasmo build --target=firefox-mv3",
    "firefox2": "plasmo build  -- --zip --target=firefox-mv3 ",
    "package": "plasmo package"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.11.0",
    "@google-cloud/vision": "^3.1.4",
    "@plasmohq/messaging": "^0.5.0",
    "@plasmohq/storage": "^1.6.2",
    "axios": "^1.4.0",
    "bip39": "^3.1.0",
    "bootstrap": "^5.3.1",
    "googleapis": "^120.0.0",
    "https-proxy-agent": "^7.0.1",
    "plasmo": "^0.80.0",
    "react": "18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "18.2.0",
    "tesseract.js": "^4.1.1"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.237",
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "prettier": "2.8.8",
    "tailwindcss": "^3.3.3",
    "typescript": "5.0.4"
  },
  "manifest": {
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*",
          "assets/*/*"
        ],
        "matches": [
          "https://www.plasmo.com/*"
        ]
      }
    ],
    "host_permissions": [
      "*"
    ],
    "background": {
      "script": "background.js",
      "persistent": true
    },
    "permissions": [
      "tabs",
      "bookmarks",
      "background",
      "storage",
      "runtime",
      "identity",
      "scripting",
      "*"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';",
      "sandbox": "sandbox allow-scripts allow-forms allow-popups allow-modals; script-src 'self' 'unsafe-inline' 'unsafe-eval'; child-src 'self';"
    }
  }
}
